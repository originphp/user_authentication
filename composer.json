{
    "name": "originphp/user_authentication",
    "description": "Custom plugin for OriginPHP",
    "type": "origin-plugin",
    "license": "MIT",
    "require": {
        "originphp/framework": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    },
    "autoload": {
        "psr-4": {
            "UserAuthentication\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UserAuthentication\\Test\\": "tests/",
            "Origin\\Test\\": "vendor/originphp/framework/tests/"
        }
    }
}