{
    "name": "originphp/user_authentication",
    "description": "User Authentication Plugin for OriginPHP",
    "type": "originphp-plugin",
    "license": "MIT",
    "require": {
        "originphp/framework": "^3.0",
        "originphp/filesystem": "^1.0",
        "originphp/plugin-installer": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2"
    },
    "autoload": {
        "psr-4": {
            "UserAuthentication\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UserAuthentication\\Test\\": "tests/"
        }
    },
    "extra": {
        "install": "plugins/user_authentication"
     }
}